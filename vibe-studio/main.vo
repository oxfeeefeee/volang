package main

import "./system"

func main() {
    system.Run(
        "vibe-studio/ui/app.detra",
        "Vibe Studio",
        800,
        600,
        handleCommand,
    )
}

func handleCommand(name string, getArg func(string) string) {
    switch name {
    case "Log":
        println("[Detra Log]", getArg("message"))
    default:
        println("[Unknown command]", name)
    }
}
