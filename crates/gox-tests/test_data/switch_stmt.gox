// Test: switch statement
package main

func main() {
	// Basic switch with tag
	x := 2
	result := 0
	switch x {
	case 1:
		result = 10
	case 2:
		result = 20
	case 3:
		result = 30
	default:
		result = -1
	}
	assert(result == 20, "switch case 2 should set result to 20")
	
	// Switch with multiple case values
	y := 5
	category := 0
	switch y {
	case 1, 2, 3:
		category = 1
	case 4, 5, 6:
		category = 2
	default:
		category = 3
	}
	assert(category == 2, "switch case 4,5,6 should set category to 2")
	
	// Default case
	z := 100
	hit := 0
	switch z {
	case 1:
		hit = 1
	case 2:
		hit = 2
	default:
		hit = 99
	}
	assert(hit == 99, "switch default should set hit to 99")
	
	println("switch tests passed")
}

