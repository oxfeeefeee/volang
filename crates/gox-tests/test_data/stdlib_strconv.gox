package main

import "strconv"

func main() {
    // Test basic formatting without string comparison (due to string equality issues)
    // Just verify they don't panic and produce non-empty results
    
    s1 := strconv.Itoa(42)
    assert(len(s1) > 0, "Itoa should return non-empty string")
    
    s2 := strconv.FormatInt(255, 16)
    assert(len(s2) > 0, "FormatInt should return non-empty string")
    
    s3 := strconv.FormatBool(true)
    assert(len(s3) > 0, "FormatBool should return non-empty string")
    
    println("All strconv tests passed!")
}
