// Test method with value receiver
package main

type Counter struct {
    value int
}

func (c Counter) Get() int {
    return c.value
}

func (c Counter) Add(x int) int {
    return c.value + x
}

func main() {
    c := Counter{value: 10}
    
    _ = c.Get()
    _ = c.Add(5)
}

