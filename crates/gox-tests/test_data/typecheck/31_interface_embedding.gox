// +vm
// Test embedded interface in interface
package main

type Reader interface {
    Read() int
}

type Writer interface {
    Write(x int)
}

type ReadWriter interface {
    Reader
    Writer
}

type File struct {
    data int
}

func (f File) Read() int {
    return f.data
}

func (f File) Write(x int) {
    f.data = x
}

func main() {
    var rw ReadWriter
    f := File{data: 0}
    
    rw = f  // File implements ReadWriter
    rw.Write(42)
    _ = rw.Read()
}

