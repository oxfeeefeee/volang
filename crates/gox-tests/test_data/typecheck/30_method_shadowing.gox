// +vm
// Test method shadowing - outer method shadows embedded method
package main

type Inner struct {
    value int
}

func (i Inner) Get() int {
    return i.value
}

type Outer struct {
    Inner
}

func (o Outer) Get() int {
    return o.value * 2
}

func main() {
    o := Outer{Inner: Inner{value: 10}}
    
    // Outer.Get shadows Inner.Get
    _ = o.Get()        // calls Outer.Get
    _ = o.Inner.Get()  // calls Inner.Get explicitly
}

