// Test embedded struct promotes methods
package main

type Inner struct {
    value int
}

func (i Inner) GetValue() int {
    return i.value
}

type Outer struct {
    Inner  // embedded
    name string
}

func main() {
    o := Outer{Inner: Inner{value: 42}, name: "test"}
    
    // GetValue is promoted from Inner to Outer
    _ = o.GetValue()
    _ = o.Inner.GetValue()
}

=== typecheck ===
OK
