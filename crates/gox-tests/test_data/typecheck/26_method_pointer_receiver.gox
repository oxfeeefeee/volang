// Test method chaining
package main

type Builder struct {
    value int
}

func (b Builder) SetValue(x int) Builder {
    b.value = x
    return b
}

func (b Builder) GetValue() int {
    return b.value
}

func main() {
    b := Builder{value: 0}
    
    // Method chaining
    result := b.SetValue(10).GetValue()
    _ = result
}

