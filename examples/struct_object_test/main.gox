package main

// Test struct (value type) vs object (reference type) semantics

type S struct {
    x int
    y int
}

type O object {
    x int
    y int
}

// More complex types with multiple field types
type Person struct {
    age    int
    height int
    score  int
}

type ObjPerson object {
    age    int
    height int
    score  int
}


func main() {
    // Test 1: Struct value semantics
    println(1000)
    s1 := S{10, 20}
    s2 := s1
    s2.x = 100
    println(s1.x)  // 10
    println(s2.x)  // 100
    
    // Test 2: Object reference semantics
    println(2000)
    o1 := O{10, 20}
    o2 := o1
    o2.x = 100
    println(o1.x)  // 100
    println(o2.x)  // 100
    
    // Test 3: Multi-field struct
    println(3000)
    p := Person{11, 22, 33}
    println(p.age)     // 11
    println(p.height)  // 22
    println(p.score)   // 33
    
    println(9999)
}
