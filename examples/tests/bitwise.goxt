# Bitwise operators test

module bitwise

func main
  locals 10
  # r0 = 0b1010 = 10
  0: LoadInt r0, 10
  # r1 = 0b1100 = 12
  1: LoadInt r1, 12
  # r2 = r0 & r1 = 0b1000 = 8
  2: Band r2, r0, r1
  3: DebugPrint r2
  # r3 = r0 | r1 = 0b1110 = 14
  4: Bor r3, r0, r1
  5: DebugPrint r3
  # r4 = r0 ^ r1 = 0b0110 = 6
  6: Bxor r4, r0, r1
  7: DebugPrint r4
  # r5 = 1
  8: LoadInt r5, 1
  # r6 = r0 << 1 = 20
  9: Shl r6, r0, r5
  10: DebugPrint r6
  # r7 = r0 >> 1 = 5
  11: Shr r7, r0, r5
  12: DebugPrint r7
  13: Return r0, 0
end

entry main
