package main

import "fmt"

func getIntAndInt() (int, int) {
	return 42, 100
}

func main() {
	fmt.Println("Test: Multi-assign (int, int) to (any, any)")
	var i1, i2 any
	i1, i2 = getIntAndInt()
	fmt.Println("i1:", i1, "i2:", i2)
	fmt.Println("i1 == 42:", i1 == 42)
	fmt.Println("i2 == 100:", i2 == 100)
	
	// This should work but panics
	v1 := i1.(int)
	v2 := i2.(int)
	fmt.Println("v1:", v1, "v2:", v2)
	
	fmt.Println("PASSED")
}
