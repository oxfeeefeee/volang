package main

// Test: package-level interface variable initialization
// Bug: concrete type assigned to interface at package level causes segfault

type Writer interface {
	Write(p []byte) (n int, err error)
}

type devNull struct{}

func (d *devNull) Write(p []byte) (int, error) {
	return len(p), nil
}

var Discard Writer = &devNull{}

func main() {
	// Test global interface init
	n, _ := Discard.Write([]byte("hello"))
	assert(n == 5)
}
